<?xml version="1.0" encoding="utf-8"?>
<config>
    <server_ip>192.168.2.123</server_ip>
    <http_enable>1</http_enable>
    <http_port>10000</http_port>
    <https_enable>0</https_enable>
    <https_port>8443</https_port>
    <cert_file>ssl.ca</cert_file>
    <key_file>ssl.key</key_file>
    <http_max_users>16</http_max_users>
    <need_auth>0</need_auth>
    <log_enable>1</log_enable>
    <log_level>1</log_level>
    <information>
        <Manufacturer>Videopos</Manufacturer>
        <Model>IPKassa</Model>
        <FirmwareVersion>2.4</FirmwareVersion>
        <SerialNumber>help@romka.kz</SerialNumber>
        <HardwareId>1.0</HardwareId>
    </information>
    <user>
        <username>admin</username>
        <password>admin</password>
        <userlevel>Administrator</userlevel>
    </user>
    <profile>
        <video_source>
            <width>1920</width>
            <height>1080</height>
        </video_source>
        <video_encoder>
            <width>1920</width>
            <height>1080</height>
            <quality>16</quality>
            <session_timeout>10</session_timeout>			
            <framerate>20</framerate>
            <encoding_interval>20</encoding_interval>
            <bitrate_limit>3072</bitrate_limit>
            <encoding>H264</encoding>
            <h264>
                <gov_length>25</gov_length>
                <h264_profile>Main</h264_profile>
            </h264>
        </video_encoder>
       
        <stream_uri append_params="1">rtsp://192.168.2.123/screenlive</stream_uri>
    </profile>	

    <scope>onvif://www.onvif.org/location/country/china</scope>
    <scope>onvif://www.onvif.org/name/IP-Camera</scope>
    <scope>onvif://www.onvif.org/hardware/HI3518C</scope>	
    <event>
        <renew_interval>60</renew_interval>	
        <simulate_enable>1</simulate_enable>
    </event>		
</config>

